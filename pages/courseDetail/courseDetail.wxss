page{
  background-color: white;
}
.title{
    padding-left: 15px;
}
.name{
    font-size: 16px;
    font-weight: bolder;
    margin-top: 10px;
}
.des{
    margin-top: 10px;
    color: #353535;
    font-size: 14px;
    width:85%;
    display: -webkit-box;
  -webkit-box-orient:vertical;
  text-overflow: ellipsis;
  overflow:hidden; 
}
.ellipsis{
    -webkit-line-clamp:2;
  }
  .unellipsis{
    -webkit-line-clamp:0; 
  }

  .desBox{
      display: flex;
      flex-direction: row;
      align-items: flex-end;
  }
  .fold{
      font-size: 12px;
      color: #353535;
  }

  .score{
      
      margin-top: 10px;
      margin-right: 10px;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center; 
  }
 
  .Cscore{
      font-size: 16px;
      font-weight: bolder;
  }

  .num{
      font-size: 12px;
      color: #888888;
  }
  .scoreBox{
    display: flex;
    flex-direction: row;
    align-items: center;
  }
 
  .courseBox{
       width:90%;
       margin-top: 10px;
       padding: 10px;
  }

  .pingfen{
      display: flex;
      flex-direction: row;
      padding: 10px;
         
  }

  .textQ{
      font-size: 14px;
      margin-right: 10px;
      border-radius: 10%;
       background-color: #c90027;
      color:white;
      width: 150rpx;
      height:50rpx;
      line-height: 50rpx;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center; 
  }

  .teacher{
    display: flex;
    flex-direction: row;
  }

  .teacherBox{
      padding: 5px;
      margin-right: 10px;
      border-bottom: #888888 1px solid;

  }